{
  "publisher": "Tlntin",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tlntin/novel-copilot.git"
  },
  "name": "novel-copilot",
  "displayName": "Novel-Copilot",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "novel-copilot.helloWorld",
        "title": "Run Novel Copilot"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Novel Copilot Extension",
      "properties": {
        "server.address": {
          "type": "string",
          "default": "localhost",
          "description": "The address of the server."
        },
        "server.port": {
          "type": "number",
          "default": 6288,
          "description": "The port of the server."
        },
        "server.generated_length": {
          "type": "number",
          "default": 256,
          "description": "How much text is expected to be generated at one time"
        },
        "server.chunk_length": {
          "type": "number",
          "default": 512,
          "description": "Divide the data into blocks for reasoning, which reduces memory usage, but reduces the speed of reasoning."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/request": "^2.48.8",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "request": "^2.88.2",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "request": "^2.88.2"
  }
}
